import * "Prelude"
interface Functor<F<_>> {
  map: forall A B. function(F<A>, function(A) -> B) -> F<B>;
}
function optMap<A, B>(o: Option<A>, f: function(A) -> B) -> Option<B> {
  return match o {
    Some(x) => some(f(x)),
    None => none()
  };
}
implementation Functor<Option> {
  map = optMap;
}
