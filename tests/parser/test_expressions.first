// Test expressions and operators

function arithmetic() -> Int {
    let a = 10;
    let b = 20;
    let result = (a + b) * 2 / 3;
    return result;
}

function comparisons() -> Bool {
    let x = 5;
    let y = 10;
    return x < y && y > 0;
}

function logical() -> Bool {
    let p = true;
    let q = false;
    return p || q && !q;
}
